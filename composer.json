{
    "name": "sherriw/composerexample",
    "description": "Composer Example",
    "authors": [
        {
            "name": "Sherri Wheeler"
        }
    ],
    "require": {
        "pimple/pimple": "^3.0",
        "monolog/monolog": "^1.23",
        "syntaxseed/templateseed": "~1.0",
        "teamtnt/tntsearch": "^1.3",
        "syntaxseed/iplimiter": "^1.0"
    },
    "require-dev": {
        "brainmaestro/composer-git-hooks": "^2.6"
    },
    "autoload": {
        "classmap": [],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "cghooks": "vendor/bin/cghooks",
        "post-install-cmd": "vendor/bin/cghooks add --ignore-lock",
        "post-update-cmd": "vendor/bin/cghooks update"
    },
    "extra": {
        "hooks": {
            "pre-commit": [
                "set -- phpcsfixer phpcpd phpmd phpunit",
                ". \"hooks/git/precommit.sh\""
            ]
        }
    }
}
